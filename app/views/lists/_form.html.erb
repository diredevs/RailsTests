<hr>

<%= simple_form_for(@list, html: {class: 'form-vertical' }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :subject %>
  </div>

  <br>

  <div class="form-actions">
    <%= f.button :submit, class:"btn btn-success", id:"saveList" %>
  </div>
<% end %>

<hr>
